#User name
user1 = input("PLayer1 name is ")
user2 = input("Player2 name is ")
#User Choice

user1_answer = input(user1 + ",Do you want to rock, paper or scissors? ")
user2_answer = input(user2 + ",Do you want to rock, paper or scissors? ")

if user1_answer == user2_answer:
    print("It is a tie")
elif user1_answer == 'rock':
    if user2_answer == 'scissors':
        print("Rock wins")
    else:
        print("Paper wins")
elif user1_answer == 'scissors':
    if user2_answer == 'paper':
        print("Scissor wins")
    else:
        print("Rock wins")
elif user1_answer == 'paper':
    if user2_answer == 'rock':
        print ("Paper wins")
    else:
        print("Scissor wins")
else:
    print("Invalid INput! You have not entered rock, paper or scissors, try again.")